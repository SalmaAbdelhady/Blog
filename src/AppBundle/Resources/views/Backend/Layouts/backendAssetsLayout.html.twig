{% block favicon %}
{% endblock %}
{% block javascriptsHead %}
{% endblock %}
{% block meta_tags %}
{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
    'bundles/app/backend/css/bootstrap.min.css'
    'bundles/app/backend/css/sb-admin.css'
    'bundles/app/backend/font-awesome/css/font-awesome.css'
    'bundles/app/backend/js/jstree/themes/default/style.min.css'
